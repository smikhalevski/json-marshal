{
  "name": "json-marshal",
  "version": "0.0.5",
  "description": "JSON serializer that can stringify and parse any data type.",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./dehydrate": "./adapter/dehydrate.js",
    "./hydrate": "./adapter/hydrate.js",
    "./adapter/array-buffer": "./adapter/array-buffer.js",
    "./adapter/date": "./adapter/date.js",
    "./adapter/error": "./adapter/error.js",
    "./adapter/map": "./adapter/map.js",
    "./adapter/regexp": "./adapter/regexp.js",
    "./adapter/set": "./adapter/set.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --project tsconfig.build.json && cp package.json README.md LICENSE.txt lib/ && cd lib && npm pkg delete scripts devDependencies",
    "clean": "rimraf lib docs coverage",
    "test": "TZ=UTC vitest run",
    "test:definitions": "tsd --typings lib/index.d.ts --files src/main/global.d.ts --files 'src/test/**/*.test-d.ts'",
    "perf": "NODE_OPTIONS=--experimental-require-module toofast",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/json-marshal.git"
  },
  "keywords": [
    "json",
    "stringify",
    "parse",
    "serialize",
    "circular",
    "cyclic"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/json-marshal/issues"
  },
  "homepage": "https://github.com/smikhalevski/json-marshal#readme",
  "devDependencies": {
    "@types/node": "^24.8.1",
    "devalue": "^5.4.1",
    "fast-json-stable-stringify": "^2.1.0",
    "flatted": "^3.3.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "toofast": "^3.0.3",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typedoc-plugin-mdn-links": "^5.0.10",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
