{
  "name": "json-marshal",
  "version": "0.0.1",
  "description": "JSON parser/serializer that preserves JS object types.",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./adapter/arrayBuffer": {
      "types": "./adapter/arrayBuffer.d.ts",
      "import": "./adapter/arrayBuffer.mjs",
      "require": "./adapter/arrayBuffer.js"
    },
    "./adapter/date": {
      "types": "./adapter/date.d.ts",
      "import": "./adapter/date.mjs",
      "require": "./adapter/date.js"
    },
    "./adapter/error": {
      "types": "./adapter/error.d.ts",
      "import": "./adapter/error.mjs",
      "require": "./adapter/error.js"
    },
    "./adapter/regexp": {
      "types": "./adapter/regexp.d.ts",
      "import": "./adapter/regexp.mjs",
      "require": "./adapter/regexp.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config && cp package.json README.md LICENSE.txt lib/ && cd lib && npm pkg delete scripts devDependencies",
    "clean": "rimraf lib docs coverage",
    "test": "jest",
    "perf": "toofast"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/json-marshal.git"
  },
  "keywords": [
    "json",
    "stringify",
    "parse",
    "serialize",
    "circular",
    "cyclic"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/json-marshal/issues"
  },
  "homepage": "https://github.com/smikhalevski/json-marshal#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "fast-json-stable-stringify": "^2.1.0",
    "flatted": "^3.3.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "rollup": "^4.17.2",
    "toofast": "^2.0.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  }
}
