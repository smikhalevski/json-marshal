{
  "name": "JSON Marshal",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "disableSources": true,
  "hideGenerator": true,
  "visibilityFilters": {
    "inherited": true,
    "protected": false
  },
  "navigation": {
    "includeFolders": false
  },
  "sortEntryPoints": false,
  "readme": "none",
  "tsconfig": "./tsconfig.json",
  "entryPoints": ["./src/main/index.ts", "./src/main/adapter/*.ts"],
  "plugin": ["typedoc-plugin-mdn-links"]
}
